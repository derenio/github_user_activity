doctype html
html
  head
    title GitHub users' activities
    link(href="/static/vis/vis.css" rel="stylesheet" type="text/css")
  body
    h1 Display analysis of:
    div(id="data-entries")
      ul
        each dataFilename, i in dataFilenames
          li
            a(href=`/analysis/activities/detail/${dataFilename}` target="_blank")=dataFilename

    h1 Display worked on of:
    div(id="worked-on-entries")
      ul
        each dataFilename, i in dataFilenames
          li
            a(href=`/analysis/activities/worked-on/${dataFilename}` target="_blank")=dataFilename
